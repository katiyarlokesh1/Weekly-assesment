

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <welcome-file-list>
	<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
	<servlet>
	<servlet-name>register</servlet-name>
	<servlet-class>com.globallogic.reg.Register</servlet-class>
	</servlet>
	<servlet-mapping>
	<servlet-name>register</servlet-name>
	<url-pattern>/reg</url-pattern>
	</servlet-mapping>
	
	<servlet>
	<servlet-name>login</servlet-name>
	<servlet-class>com.globallogic.reg.Login</servlet-class>
	</servlet>
	<servlet-mapping>
	<servlet-name>login</servlet-name>
	<url-pattern>/log</url-pattern>
	</servlet-mapping>
</web-app>
